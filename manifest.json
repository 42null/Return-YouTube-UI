{

  "manifest_version": 2,
  "name": "Youtube With The Actual Corners",
  "version": "1.0",
  "description": "Extension aimed at making youtube look like it used to before 10/27/2022 when everything became rounded and circular.",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*","*://*.example.net/*"],
      "js": ["youtubewithactualcorners.js"]
    }
  ],

  "browser_specific_settings": {
    "gecko": {
      "id": "borderify@example.com"
    }
  },
  "icons": {
    "48": "icons/border-48.png"
  }


}
