{

  "manifest_version": 2,
  "name": "Return Youtube UI",
  "version": "1.0",
  "description": "Extension aimed at making youtube look like it used to before everything became rounded and circular.",

  "icons": {
    "512": "icons/ReturnYoutubeUIIconV2R1_512.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["returnYoutubeUI.js"]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "returnYoutubeUI@example.com"
    }
  },
  "icons": {
    "48": "icons/ReturnYoutubeUIIconV2R1_512.png"
  }


}
